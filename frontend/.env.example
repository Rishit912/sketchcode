# Backend API URL used at build time by Vite.
# Set this to your deployed backend (e.g. https://your-backend.onrender.com).
# The code looks for VITE_BACKEND_URL first, then VITE_API_URL for backwards compatibility.
VITE_BACKEND_URL=http://localhost:8001

# (Optional) legacy name - some setups may still use this
VITE_API_URL=http://localhost:8001

# Enable/disable debugging features
VITE_DEBUG_MODE=false