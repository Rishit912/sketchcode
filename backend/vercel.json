{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/backend/api/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/frontend/$1",
      "check": true
    },
    {
      "source": "/(.*)",
      "destination": "/frontend/index.html"
    }
  ]
}